        "# 税込価格を計算\n",
        "def calculate_tax(price, tax_rate):\n",
        "     return price * (1 + tax_rate / 100)\n",
        "\n",
        " # 商品の金額\n",
        "price = 110\n",
        " # 消費税率（％）\n",
        "tax_rate = 10\n",
        "\n",
        "# 関数を呼び出し\n",
        "result = calculate_tax(price, tax_rate )\n",
        "# 計算結果\n",
        "print(\"税込価格:\", result)"
